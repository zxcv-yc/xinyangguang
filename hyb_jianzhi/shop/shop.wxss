.my_bg {
    height: 395rpx;
}

.my_bg .pa {
    bottom: 0;
    left: 0;
}

.header_bg {
    padding: 8rpx;
    background-color: rgba(255,255,255,.5);
    position: relative;
}

.header_bg .vipimg {
    width: 55rpx;
    height: 55rpx;
    text-align: center;
    line-height: 55rpx;
    position: absolute;
    bottom: -10%;
    left: 90%;
    transform: translateX(-50%);
    border-radius: 50%;
    font-size: 28rpx;
    color: #fff;
    background: #f4ea2a;
}

.bdcf {
    height: 44rpx;
    line-height: 44rpx;
    padding: 0 10rpx;
}

.bdcf::after {
    border: 2rpx solid #fff;
}

.h160 {
    height: 160rpx;
}

.h88 {
    height: 88rpx;
    line-height: 88rpx;
}

.yueshow {
    font-size: 28rpx;
    margin-right: 15rpx;
    color: #2c2c2c;
}

button {
    width: 100%;
    height: 100%;
    border-radius: 0rpx;
    line-height: 100%;
    padding: 0;
}

button::after {
    border: none;
}

.renew {
    padding: 0 10rpx;
    box-sizing: border-box;
    background: none;
    color: #fff;
    font-size: 26rpx;
    font-weight: 300;
}

.paddingNav {
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    box-sizing: border-box;
}

.zhezhao {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    background: rgba(0,0,0,0.5);
    z-index: 9999;
}

.zhe_main {
    position: absolute;
    left: 50%;
    transform: translate(-50%,-50%);
    top: 50%;
    width: 60%;
    font-size: 28rpx;
    color: #999;
    background-color: #fff;
    padding: 20rpx;
}

.zhe_main .lxfy {
    background-color: #eee;
    border-radius: 30rpx;
    font-size: 30rpx;
    margin-top: 30rpx;
    line-height: 40px;
}