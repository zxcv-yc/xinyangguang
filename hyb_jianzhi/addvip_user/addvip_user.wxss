page {
    background: #fff;
}

view {
    box-sizing: border-box;
}

image {
    width: 100%;
    height: 100%;
}

.vipsel {
    width: 100%;
    padding: 25rpx;
    font-size: 30rpx;
    color: #242424;
    font-weight: 600;
    border-bottom: 1rpx solid #eee;
}

.viplist {
    width: 100%;
    padding: 25rpx;
    border-bottom: 1rpx solid #eee;
}

.viptop {
    font-size: 28rpx;
    color: #363636;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.viptop view icon.emptyicon {
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    border: 1rpx solid #363636;
}

.viptit {
    width: 100%;
    font-size: 28rpx;
    color: #000;
    padding: 10rpx;
    padding-left: 0;
}

.vipjs view {
    width: 100%;
    word-break: break-all;
    font-size: 28rpx;
    color: #363636;
    padding-left: 40rpx;
    padding-bottom: 10rpx;
}

.btns {
    width: 96%;
    background: #ff644a;
    color: #fff;
    font-size: 30rpx;
    border-radius: 10rpx;
    position: absolute;
    bottom: 20%;
    left: 2%;
}