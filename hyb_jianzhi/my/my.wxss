page {
    height: 100%;
}

.my_bg {
    height: 395rpx;
}

.my_bg .pa {
    bottom: 0;
    left: 0;
}

.header_bg {
    width: 130rpx;
    height: 130rpx;
    background-color: rgba(255,255,255,.5);
}

.bdcf {
    height: 64rpx;
    line-height: 64rpx;
    padding: 0 10rpx;
}

.bdcf::after {
    border: 2rpx solid #fff;
}

.h160 {
    height: 160rpx;
}

.h88 {
    height: 88rpx;
    line-height: 88rpx;
}

.yueshow {
    font-size: 28rpx;
    margin-right: 15rpx;
    color: #2c2c2c;
}

button {
    width: 100%;
    height: 100%;
    border-radius: 0rpx;
    line-height: 100%;
    padding: 0;
}

button::after {
    border: none;
}

.paddingNav {
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    box-sizing: border-box;
}