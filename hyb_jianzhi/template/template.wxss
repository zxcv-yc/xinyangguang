page {
    padding-bottom: 96rpx;
}

image {
    width: 100%;
    height: 100%;
    vertical-align: middle;
}

.swiper_wrap {
    width: 100%;
    height: 340rpx;
}

.sub_nav {
    border-bottom: 2rpx solid #f1f1f1;
    height: 70rpx;
    line-height: 70rpx;
    background: #f0f0f0;
    margin: 20rpx 0;
    margin-bottom: 0rpx;
}

.sub_nav text {
    font-size: 28rpx;
    padding: 0 20rpx;
    color: #242424;
    display: inline-block;
    height: 100%;
    position: relative;
}

.sub_nav .onon {
    font-size: 30rpx;
    color: #289AF6;
}

.sub_nav .onon::after {
    content: "";
    display: block;
    height: 4rpx;
    width: 30rpx;
    background: #289AF6;
    position: absolute;
    bottom: 5rpx;
    left: 42%;
}

.footer {
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    padding: 10rpx;
    box-sizing: border-box;
    background: #fff;
    border-top: 1rpx solid #C5C5C5;
    z-index: 5555;
}

.footer view {
    display: inline-block;
}

.footer view image {
    width: 40rpx;
    height: 40rpx;
    display: block;
    margin: 0 auto;
}

.footer view text {
    display: block;
    width: 100%;
    font-size: 22rpx;
    color: #AFAFAF;
    text-align: center;
    margin-top: 5rpx;
}

.footer view .sel_color {
    color: #04a9f0;
}

.copy {
    text-align: center;
    font-size: 28rpx;
    margin-top: 30rpx;
}

.copy .fuhao {
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    border: transparent;
}

.nomore {
    line-height: 80rpx;
    height: 80rpx;
    text-align: center;
    position: relative;
    font-size: 30rpx;
    width: 100%;
    background: #f0f0f0;
    color: #B3B3B3;
}

.nomore::before {
    content: "";
    display: block;
    width: 100rpx;
    height: 2rpx;
    background: #B3B3B3;
    position: absolute;
    left: 180rpx;
    top: 50%;
}

.nomore::after {
    content: "";
    display: block;
    width: 100rpx;
    height: 2rpx;
    background: #B3B3B3;
    position: absolute;
    right: 180rpx;
    top: 50%;
}